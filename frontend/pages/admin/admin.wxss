/* pages/admin/admin.wxss */
.square{
  width: 24px;
  height: 24px;
  background-repeat: no-repeat;
  background-size: 100% 100%;
}
.small_title{
  width: 100%;
  height: 80rpx;
  display: flex;
  margin-bottom: 20rpx;
  align-items: center;
  flex-direction: column;
  font-size: large;
}
.detail {
  width: 90%;
  color: #999999;
  display: flex;
  padding: 10rpx;
  align-items: center;
  justify-content: left;
}
.which {
  color: #000;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  font-size: large;
  width: 70%;
}
.title {
  margin-bottom: 30rpx;
  font-size: x-large;
  font-weight: bold;
}
.text{
  width: 90%;
  height: 75rpx;
  display: flex;
  align-items: center;
}
.haveselect{
  background-color: #dddddd;
}
.pic {
  color: #660874;
  border: 2px solid #660874;
}
.pics {
  color: #ffffff;
  background-color: #660874;
}